.container{
  height:300px;
  width:100%;
}
.bg{
  position: absolute;
  z-index:0;
  height: 300px;
  width:300px;
}
button{
  margin: 10px 20px 0px 20px;
}
.hat{
  height: 100px;
  width: 100px;
  position: absolute;
  border: dashed 2px yellow;
  top:100px;
}
.cloth{
  height: 100px;
  width: 100px;
  position: absolute;
  border: dashed 2px yellow;
  top:100px;
}
.handle,.cancel{
  position: absolute;
  z-index: 1;
  width:20px;
  height:20px;
}
.scrollView{
   width: 100%;
   /* position: absolute;
   bottom: 5px; */
   margin-top: 50rpx;
   white-space: nowrap;
}
.imgList{
  height: 70px;
  width: 70px;
  border:2px solid;
  border-color: #eeeeee;
  margin: 5px;
}
.wrap{
  position:relative;
  padding:20rpx;
  width:100%;
  height: 100%;

  box-sizing: border-box;
}
